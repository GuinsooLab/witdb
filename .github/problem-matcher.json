{
  "problemMatcher": [
    {
      "owner": "Maven - Error Prone and Checkstyle",
      "pattern": [
        {
          "regexp": "^.*\\[(ERROR)\\]\\s+(.*):\\[(\\d+),(\\d+)\\] (?:error: )?[\\[\\(](.*)[\\]\\)] (.*)$",
          "severity": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "message": 6,
          "code": 5
        }
      ]
    }
  ]
}
